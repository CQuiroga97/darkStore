{
    "family": "inventario-task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::654654383573:role/aws-service-role/ecs.amazonaws.com/AWSServiceRoleForECS",
    "containerDefinitions": [
      {
        "name": "inventario-dev",
        "image": "654654383573.dkr.ecr.us-east-1.amazonaws.com/darkstore-repo:inventario-dev",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 3000,
            "hostPort": 3000
          }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-group": "/ecs/inventario-dev",
              "awslogs-region": "us-east-1",
              "awslogs-stream-prefix": "ecs"
            }
          }
      }
    ],
    
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
  }